class User {
  depend java.io.Serializable;
  isA Serializable;
  private static final long serialVersionUID = 2315072607928790502L;
  
  
  public static void reinitializeUniqueEmail(Administrator administrator, List<Guide> guides, List<Member> members) {
    usersByEmail = new HashMap<String, User>();
  
  	usersByEmail.put(administrator.getEmail(), administrator);
    for (Guide g : guides) {
      usersByEmail.put(g.getEmail(), g);
    }
    for (Member m : members) {
      usersByEmail.put(m.getEmail(), m);
    }
  }
}

class BookableItem {
  depend java.io.Serializable;
  isA Serializable;
  private static final long serialVersionUID = 8896099581655989380L;
  
  public static void reinitializeUniqueName(List<Equipment> equipments, List<EquipmentBundle> equipmentBundles) {
    bookableitemsByName = new HashMap<String, BookableItem>();
  
    for (Equipment e : equipments) {
      bookableitemsByName.put(e.getName(), e);
    }
    for (EquipmentBundle b : equipmentBundles) {
      bookableitemsByName.put(b.getName(), b);
    }
  }
}

class Hotel {
  depend java.io.Serializable;
  isA Serializable;
  private static final long serialVersionUID = 8896099581655989382L;
  
  public static void reinitializeUniqueName(List<Hotel> hotels) {
    hotelsByName = new HashMap<String, Hotel>();
    for (Hotel h : hotels) {
      hotelsByName.put(h.getName(), h);
    }
  }
}

